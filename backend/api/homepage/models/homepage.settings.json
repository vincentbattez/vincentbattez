{
  "kind": "singleType",
  "connection": "default",
  "collectionName": "homepages",
  "info": {
    "name": "homepage"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "subTitle": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "cv": {
      "type": "component",
      "repeatable": false,
      "component": "link.link",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    }
  }
}
